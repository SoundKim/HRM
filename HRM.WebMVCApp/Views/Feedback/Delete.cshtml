@model FeedbackResponseModel
<div class="container">
    <form asp-controller="Feedback" asp-action="Delete" method="post">
        <input type="hidden" asp-for="Id" />
        <div class="my-5 row col-12">
            <label for="InterviewId">InterviewId</label>
            <select asp-for="InterviewId" class="form-control" disabled>
                @foreach (var item in ViewBag.InterviewList)
                {
                    <option value="@item.Value">@item.Text</option>
                }
            </select>
            <span asp-validation-for="InterviewId"></span>
        </div>
        <div class="my-5 row col-12">
            <label for="Description">Description</label>
            <input asp-for="Description" class="form-control" disabled />
            <span asp-validation-for="Description"></span>
        </div>
        <div class="my-5 row col-12">
            <label for="Abbr">Abbr</label>
            <input asp-for="Abbr" class="form-control" disabled />
            <span asp-validation-for="Abbr"></span>
        </div>
        <input type="submit" value="Add Feedback" class="btn btn-danger" />
    </form>
</div>