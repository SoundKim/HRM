@model EmployeeTypeResponseModel
<div class="container">
    <form asp-controller="EmployeeType" asp-action="Delete" method="post">
        <div class="my-5 row col-12">
            <label for="Title">Title</label>
            <input asp-for="Title" class="form-control" disabled />
            <span asp-validation-for="Title"></span>
        </div>
        <div class="my-5 row col-12">
            <label for="Description">Description</label>
            <input asp-for="Description" class="form-control" disabled />
            <span asp-validation-for="Description"></span>
        </div>
        <div class="my-5 row col-12 form-check">
            <label for="IsActive">IsActive</label>
            <input type="checkbox" class="form-check-input" asp-for="IsActive" disabled>
            <span asp-validation-for="IsActive"></span>
        </div>
        <input type="submit" value="Add Employee Type" class="btn btn-danger" />
    </form>
</div>